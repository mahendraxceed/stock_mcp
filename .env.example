# Server
PORT=3001

# Upstox (https://account.upstox.com/developer/apps)
UPSTOX_API_KEY=your_client_id
UPSTOX_API_SECRET=your_client_secret
UPSTOX_REDIRECT_URI=http://localhost:3001/auth/upstox/callback
UPSTOX_ACCESS_TOKEN=                # Optional — obtained via OAuth at /auth/upstox

# Rupeezy (https://vortex.rupeezy.in/login)
RUPEEZY_APPLICATION_ID=your_app_id
RUPEEZY_API_SECRET=your_api_secret
RUPEEZY_REDIRECT_URI=http://localhost:3001/auth/rupeezy/callback
RUPEEZY_ACCESS_TOKEN=               # Optional — obtained via OAuth at /auth/rupeezy

# Optional
LOG_LEVEL=info
CACHE_TTL_SECONDS=30
